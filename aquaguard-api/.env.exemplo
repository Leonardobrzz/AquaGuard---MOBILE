# Configuração para desenvolvimento local do AquaGuard API

# Banco de dados H2 (em memória)
DB_URL=jdbc:h2:mem:aquaguard_dev;DB_CLOSE_DELAY=-1;DB_CLOSE_ON_EXIT=FALSE
DB_USER=sa
DB_PASS=

# JWT Secret para desenvolvimento
JWT_SECRET=AquaGuardSecretKeyForDevelopment2024!@#

# Email para desenvolvimento (usar log do console)
MAILTRAP_API_USER=dummy
MAILTRAP_API_TOKEN=dummy

# Para usar com Spring Boot, renomeie este arquivo para .env
# ou configure as variáveis no sistema
